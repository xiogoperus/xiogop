{
    "name": "xiogop",
    "version": "1.2.2",
    "description": "Xiogop is an open source MVC Mini Framework provided free for use under the GNU General Public License.",
    "main": "index.js",
    "scripts": {
        "watch-site": "webpack -w --config webpack.public.config.js",
        "watch-admin": "webpack -w --config webpack.admin.config.js",
        "build-site": "webpack -p --config webpack.public.config.js",
        "build-admin": "webpack -p --config webpack.admin.config.js",
        "ipack": "cd ./public && bower install --allow-root"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xiogoperus/xiogop.git"
    },
    "keywords": [
        "MVC",
        "mini",
        "Framework"
    ],
    "author": "Xiogoperus",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/xiogoperus/xiogop/issues"
    },
    "homepage": "https://github.com/xiogoperus/xiogop#readme",
    "devDependencies": {
        "extract-text-webpack-plugin": "^3.0.1",
        "file-loader": "^1.1.5",
        "css-loader": "^0.28.7",
        "style-loader": "^0.19.0",
        "url-loader": "^0.6.2",
        "less": "^2.7.2",
        "less-loader": "^4.0.5",
        "webpack": "^3.6.0",
        "bower": "^1.8.2"
    }
}